    <artifactId>spring-family_play</artifactId>
    <version>0.0.1</version>
    <modules>
        <module>datasource-demo</module>
        <module>multi-datasource-demo</module>
        <module>druid-demo</module>
        <module>jdbc-template-demo</module>
        <module>transaction-demo</module>
        <module>database-error-code-demo</module>
        <module>jpa-demo</module>
        <module>mybatis-demo</module>
    </modules>
    <dependencyManagement>
